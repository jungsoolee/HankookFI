  object ADOQuery_SCMELATT: TADOQuery
    Connection = DataModule_SettleNet.ADOConnection_Main
    Parameters = <>
    SQL.Strings = (
      
        '(Select ad.PARTY_ID, pt.PARTY_NAME_KOR, MEDIA_NO = pa.MEDIA_NO, ' +
        'pa.RCV_NAME_KOR,'
      
        '        ad.DIRECT_SEND, ad.REPORT_ID, t.ACC_NO, t.DGT_SGNT, ac.A' +
        'CC_NAME_KOR'
      
        '      From SETRADE_TBL t, SEADBOK_TBL ad, SCPRTAD_TBL pa, SEACBI' +
        'F_TBL ac, SCPARTY_TBL pt'
      '      Where (t.TRADE_DATE = '#39'20000418'#39
      '        and (t.ACC_NO = ad.ACC_NO)'
      '        and (t.SUB_ACC_NO = ad.SUB_ACC_NO)'
      '        and (ad.PARTY_ID = pa.PARTY_ID)'
      '        and (ad.SEND_MTD = pa.SEND_MTD)'
      '        and (ad.SEND_SEQ = pa.SEND_SEQ)'
      '        and (ad.PARTY_ID = pt.PARTY_ID)'
      '        and (ad.GLOBAL_YN = '#39'Y'#39')'
      '        and (ac.ACC_NO = t.ACC_NO)'
      '        and (ad.SEND_MTD = '#39'2'#39')'
      'UNION'
      
        '      Select ad.PARTY_ID, PARTY_NAME_KOR = u.RCV_NAME_KOR, MEDIA' +
        '_NO = u.MEDIA_NO, u.RCV_NAME_KOR,'
      
        '        ad.DIRECT_SEND, ad.REPORT_ID, t.ACC_NO, t.DGT_SGNT, ac.A' +
        'CC_NAME_KOR'
      
        '      From SETRADE_TBL t, SEADBOK_TBL ad, SUPRTAD_TBL u, SEACBIF' +
        '_TBL ac'
      '      Where (t.TRADE_DATE = '#39'20000418'#39
      '        and (t.ACC_NO = ad.ACC_NO)'
      '        and (t.SUB_ACC_NO = ad.SUB_ACC_NO)'
      '        and (ad.SEND_MTD = u.SEND_MTD)'
      '        and (ad.SEND_SEQ = u.SEND_SEQ)'
      '        and (ad.GLOBAL_YN = '#39'N'#39')'
      '        and (ac.ACC_NO = t.ACC_NO )'
      '        and (ad.SEND_MTD = '#39'2'#39'))'
      ' Order By PARTY_NAME_KOR, MEDIA_NO, t.ACC_NO')
    Left = 184
    Top = 11
  end
